{
  "usesTypeScript": false,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/component/TemplateName.jsx",
        "style": "templates/component/TemplateName.scss",
        "test": "templates/component/TemplateName.test.jsx"
      },
      "path": "src/components",
      "withStyle": true,
      "withTest": true,
      "withStory": false,
      "withLazy": false
    },
    "page": {
      "customTemplates": {
        "component": "templates/page/TemplateName.jsx",
        "style": "templates/page/TemplateName.scss",
        "test": "templates/page/TemplateName.test.jsx"
      },
      "path": "src/pages",
      "withLazy": false,
      "withStory": false,
      "withStyle": true,
      "withTest": true
    },
    "redux-slice": {
      "customTemplates": {
        "component": "templates/redux/slice/TemplateName.slice.js"
      },
      "path": "src/redux",
      "withLazy": false,
      "withStory": false
    }
  }
}